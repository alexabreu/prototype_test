{
    /*** Globals ***/
    // To ignore any custom global variables, enable the `predef` option and list
    // your variables within it.
    "predef": [
        
    ],

    /*** Enforcing options ***/
    // Set these to `true` to enforce, or `false` to relax.
    "adsafe":     false,
    "safe":       false,

    /*** Relaxing options ***/
    // Set these to `true` to relax, or `false` to enforce.
    "bitwise":    false,
    "cap":        false,
    "confusion":  true,
    "continue":   true,
    "css":        true,
    "debug":      true,
    "eqeq":       false,
    "es5":        true,
    "evil":       false,
    "forin":      false,
    "fragment":   true,
    "newcap":     false,
    "nomen":      true,
    "on":         false,
    "plusplus":   false,
    "regexp":     true,
    "sloppy":     false,
    "sub":        false,
    "undef":      false,
    "unparam":    true,
    "vars":       true,
    "white":      false,
    "validthis":  true,
    "strict_mode":true,

    /*** Environments ***/
    // Set each environment that you're using to `true`.
    "browser":    true,
    "devel":      false,
    "node":       true,
    "rhino":      false,
    "widget":     false,
    "windows":    false,

    /*** Miscellaneous ***/
    "indent":     4,
    "maxerr":     500,
    // "maxlen":     80,
    "passfail":   false
}